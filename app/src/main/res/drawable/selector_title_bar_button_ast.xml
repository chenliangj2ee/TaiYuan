<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <solid android:color="#0066FF" />
            <stroke android:width="0px" android:color="#888888" android:dashGap="0dp" android:dashWidth="0dp" />
            <corners android:bottomLeftRadius="0dp" android:bottomRightRadius="0dp" android:topLeftRadius="0dp" android:topRightRadius="0dp" />
        </shape>
    </item>
</selector>

<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_enabled="false">
        <shape>
            <solid android:color="#8eee" />
            <stroke android:width="2px" android:color="#433eec" android:dashGap="0px" android:dashWidth="0px" />
            <corners android:bottomLeftRadius="5px" android:bottomRightRadius="5px" android:topLeftRadius="5px" android:topRightRadius="5px" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#8eee" />
            <stroke android:width="1px" android:color="#ddd" android:dashGap="0px" android:dashWidth="0px" />
            <corners android:bottomLeftRadius="5px" android:bottomRightRadius="5px" android:topLeftRadius="5px" android:topRightRadius="5px" />
        </shape>
    </item>


</selector>
